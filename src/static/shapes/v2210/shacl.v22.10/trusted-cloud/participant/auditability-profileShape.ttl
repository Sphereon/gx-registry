gax-validation:AuditabilityProfileShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:AuditabilityProfile ;
	sh:property [ sh:path trusted-cloud:auditability ;
				  sh:name "auditability" ;
				  sh:description "Is it possible for the user to request an audit?" ;
				  skos:example "'yes', 'no'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:boolean ] ;
	sh:property [ sh:path trusted-cloud:audits ;
				  sh:name "audits" ;
				  sh:description "Description of audit, which were already performed." ;
				  skos:example "'data protection audits'" ;
				  sh:order 2 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:auditor ;
				  sh:name "auditor" ;
				  sh:description "Can audits on processes and organizational procedures related to data protection and security be conducted." ;
				  skos:example "'user', 'mandated auditor', 'No specification'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.
