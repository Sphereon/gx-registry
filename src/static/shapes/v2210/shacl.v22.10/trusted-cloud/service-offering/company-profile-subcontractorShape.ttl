gax-validation:CompanyProfileSubcontractorShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:CompanyProfileSubcontractor ;
	sh:property [ sh:path trusted-cloud:employeeCount ;
				  sh:name "employee count" ;
				  sh:description "Employee count of the organization." ;
				  skos:example "'50', 'No specification'" ;
				  sh:order 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path trusted-cloud:salesVolume ;
				  sh:name "sales volume" ;
				  sh:description "Sales volume of the organization." ;
				  skos:example "'50.000.000', 'No specification'" ;
				  sh:order 2 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path trusted-cloud:category ;
				  sh:name "category" ;
				  sh:description "Sales volume of the organization." ;
				  skos:example "'Reseller', 'ISV', 'CSP', 'others'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:numberOfPublicCloudServices ;
				  sh:name "number of public cloud services" ;
				  sh:description "Publicly offered cloud services." ;
				  skos:example "'5', '10'" ;
				  sh:order 4 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path trusted-cloud:experienceCloudServices ;
				  sh:name "experience cloud services" ;
				  sh:description "Experience / knowledge of the organization in the cloud service business." ;
				  skos:example "'< 1 years', '1-5 years', '> 5 years', 'no information'" ;
				  sh:order 5 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.
