gax-validation:SubContractorShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:SubContractor ;
	sh:property [ sh:path trusted-cloud:legalInformation ;
				  sh:name "legal information" ;
				  sh:description "Legally information of sub contractor." ;
				  skos:example "''" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path trusted-cloud:organizationProfile ;
				  sh:name "organization profile" ;
				  sh:description "Details on experience / knowledge of the organization in the cloud service business." ;
				  skos:example "'(a structure object of type OrganizationProfileSubcontractor'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path trusted-cloud:independentSubCompany ;
				  sh:name "independent sub company" ;
				  sh:description "Is the sub contractor internal or his legally sovereign company?" ;
				  skos:example "'Company internal', 'Company legally sovereign'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.
