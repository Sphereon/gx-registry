@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix gax-validation:  <{{BASE_URI}}/validation#> .

@prefix trusted-cloud: <{{BASE_URI}}/trusted-cloud#> .
@prefix gax-core: <{{BASE_URI}}/core#> .
@prefix vcard: <https://www.w3.org/2006/vcard/ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix did: <https://www.w3.org/TR/did-core/#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix ids: <https://w3id.org/idsa/core/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix schema: <http://schema.org/> .

gax-validation:OperativeProcessesShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:OperativeProcesses ;
	sh:property [ sh:path trusted-cloud:serviceMamagement ;
				  sh:name "service mamagement" ;
				  sh:description "Information about efficient service management for ensuring the quality of service?" ;
				  skos:example "'(a structured object of type ServiceManagement)'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node gax-validation:ServiceManagementShape ] ;
	sh:property [ sh:path trusted-cloud:serviceAvailability ;
				  sh:name "service availability" ;
				  sh:description "Information of ensured service availability in service level agreements." ;
				  skos:example "'(a structured object of type ServiceAvailability)'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node gax-validation:ServiceAvailabilityShape ] ;
	sh:property [ sh:path trusted-cloud:backups ;
				  sh:name "backups" ;
				  sh:description "Details on backup options and implementation of data protection concepts." ;
				  skos:example "'(a structured object of type Backups)'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node gax-validation:BackupsShape ] ;
	sh:property [ sh:path trusted-cloud:provisionPossibility ;
				  sh:name "provision possibility" ;
				  sh:description "Can the customer provision the service himself?" ;
				  skos:example "'No statement', 'Customer cannot provision', 'Customer can provision'" ;
				  sh:order 4 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:support ;
				  sh:name "support" ;
				  sh:description "Information about support services and documentation." ;
				  skos:example "'(a structured object of type Support)'" ;
				  sh:order 5 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:node gax-validation:SupportShape ] ;
.

gax-validation:SupportShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:Support ;
	sh:property [ sh:path trusted-cloud:description ;
				  sh:name "description" ;
				  sh:description "Description of the scope of support services" ;
				  skos:example "'Freetext'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:responseTimeOnNormalRequests ;
				  sh:name "response time on normal requests" ;
				  sh:description "What is the guaranteed response time on normal requests?" ;
				  skos:example "'No statement', '4 hours', '1 work day', '<3 work days'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:responseTimeOnCriticalRequests ;
				  sh:name "response time on critical requests" ;
				  sh:description "What is the guaranteed response time on critical requests?" ;
				  skos:example "'No statement', '4 hours', '1 work day', '<3 work days'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:mainResolveTimeOfMinorProblems ;
				  sh:name "main resolve time of minor problems" ;
				  sh:description "Whats the average time until minor problems are resolved?" ;
				  skos:example "'No statement', '<1 work day', '1 work day', '<4 work days'" ;
				  sh:order 4 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:mainResolveTimeMajorProblems ;
				  sh:name "main resolve time major problems" ;
				  sh:description "What is the average time until major problems are resolved?" ;
				  skos:example "'No statement', '<1 work day', '1 work day', '<4 work days'" ;
				  sh:order 5 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:availabilityOfSupport ;
				  sh:name "availability of support" ;
				  sh:description "What is the availability of the support?" ;
				  skos:example "'No statement', '5x8', '24/7'" ;
				  sh:order 6 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:userDocumentationCompleteness ;
				  sh:name "user documentation completeness" ;
				  sh:description "Is there a complete user documentation?" ;
				  skos:example "'No statement', 'Complete and updated', 'Regularly updated', 'Updated version online'" ;
				  sh:order 7 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:systemDocumentationCompleteness ;
				  sh:name "system documentation completeness" ;
				  sh:description "Is there a complete system documentation?" ;
				  skos:example "'No statement', 'Complete and updated', 'On request'" ;
				  sh:order 8 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:trainingOffered ;
				  sh:name "training offered" ;
				  sh:description "Are trainings offered?" ;
				  skos:example "'Yes', 'No'" ;
				  sh:order 9 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:boolean ] ;
	sh:property [ sh:path trusted-cloud:trainingPartners ;
				  sh:name "training partners" ;
				  sh:description "Specification from training partners" ;
				  skos:example "'Freetext'" ;
				  sh:order 10 ;
				  sh:datatype xsd:string ] ;
.

gax-validation:BackupsShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:Backups ;
	sh:property [ sh:path trusted-cloud:bckupOptions ;
				  sh:name "bckup options" ;
				  sh:description "Description of backup options" ;
				  skos:example "'Freetext'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:confideniality ;
				  sh:name "confideniality" ;
				  sh:description "Is it guaranteed, that data protection is also extended to the backup?" ;
				  skos:example "'Freetext'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.

gax-validation:ServiceManagementShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:ServiceManagement ;
	sh:property [ sh:path trusted-cloud:serviceManegent ;
				  sh:name "service manegent" ;
				  sh:description "Is an efficient service management ensured e.g. as per ITIL?" ;
				  skos:example "'No statement', 'Formally described', 'According to accepted procedure'', 'Certified', 'Certified and checked regularly'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:details ;
				  sh:name "details" ;
				  sh:description "Details about the service management" ;
				  skos:example "'Freetext'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:certificates ;
				  sh:name "certificates" ;
				  sh:description "Details about certificates, which are relevant for this service." ;
				  skos:example "'(a structured object of type Certificate'" ;
				  sh:order 3 ;
				  sh:node gax-validation:CertificateScopeShape ] ;
.

gax-validation:ServiceAvailabilityShape
	a sh:NodeShape ;
	sh:targetClass trusted-cloud:ServiceAvailability ;
	sh:property [ sh:path trusted-cloud:availability ;
				  sh:name "availability" ;
				  sh:description "Assured Service Availability in Percent / Year" ;
				  skos:example "'No statement', '99%', '99.9%', '99.99%', '99.999%', '99.999%'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:maximumDownTime ;
				  sh:name "maximum down time" ;
				  sh:description "Maximum downtime of the service in hours" ;
				  skos:example "'Freetext'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path trusted-cloud:timeToRecover ;
				  sh:name "time to recover" ;
				  sh:description "How is the quick recovery of availability of client data and access to it guarantieed in the event of a physical or technical incident?" ;
				  skos:example "'Freetext'" ;
				  sh:order 3 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.
