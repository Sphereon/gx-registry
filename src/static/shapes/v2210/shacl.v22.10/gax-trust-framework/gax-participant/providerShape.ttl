@prefix schema: <http://schema.org/> .

gax-validation:ProviderShape
	a sh:NodeShape ;
	sh:targetClass gax-trust-framework:Provider ;
	sh:property [ sh:path gax-trust-framework:hasLocations ;
				  sh:name "has locations" ;
				  sh:description "Ids of available Locations for this provider" ;
				  skos:example "'did:web:provider.com/location#1', 'did:web:provider.com/location#2'" ;
				  sh:order 1 ;
				  sh:nodeKind sh:IRI ] ;
	sh:property [ sh:path gax-trust-framework:hasServiceOffering ;
				  sh:name "has service offering" ;
				  sh:description "Ids of the ServiceOffering managed by this provider." ;
				  skos:example "'did:web:provider.com/serviceoffering#1', 'did:web:provider.com/serviceoffering#2'" ;
				  sh:order 2 ;
				  sh:nodeKind sh:IRI ] ;
.
