gax-validation:VerifiableCredentialWalletShape
	a sh:NodeShape ;
	sh:targetClass gax-trust-framework:VerifiableCredentialWallet ;
	sh:property [ sh:path gax-trust-framework:verifiableCredentialExportFormat ;
				  sh:name "verifiable credential export format" ;
				  sh:description "a list of machine readable format used to export verifiable credentials." ;
				  skos:example "'https://gaia-x.eu'" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-trust-framework:privateKeyExportFormat ;
				  sh:name "private key export format" ;
				  sh:description "a list of machine readable format used to export private keys." ;
				  skos:example "'https://gaia-x.eu'" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:datatype xsd:string ] ;
.
