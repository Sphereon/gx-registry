@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix gax-validation:  <http://w3id.org/gaia-x/validation#> .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix gax-core: <http://w3id.org/gaia-x/core#> .
@prefix gax-node: <http://w3id.org/gaia-x/node#> .
@prefix gax-resource: <http://w3id.org/gaia-x/resource#> .
@prefix gax-participant: <http://w3id.org/gaia-x/participant#> .
@prefix gax-service: <http://w3id.org/gaia-x/service#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix did: <https://www.w3.org/TR/did-core/#> .

gax-validation:NetworkingDeviceShape
	a sh:NodeShape ;
	sh:targetClass gax-resource:NetworkingDevice ;
	sh:property [ sh:path gax-resource:managementPort ;
				  sh:name "management port" ;
				  sh:order 1 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:consolePort ;
				  sh:name "console port" ;
				  sh:order 2 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:portCapacity_A ;
				  sh:name "port capacity_ a" ;
				  sh:order 3 ;
				  sh:datatype xsd:float ] ;
	sh:property [ sh:path gax-resource:portCapacity_A_Count ;
				  sh:name "port capacity_ a_ count" ;
				  sh:order 4 ;
				  sh:datatype xsd:integer ] ;
	sh:property [ sh:path gax-resource:redundantPowerSupply ;
				  sh:name "redundant power supply" ;
				  sh:order 5 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:ramSize ;
				  sh:name "ram size" ;
				  sh:order 6 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:cpuCount ;
				  sh:name "cpu count" ;
				  sh:order 7 ;
				  sh:node gax-validation:MeasureShape ] ;
	sh:property [ sh:path gax-resource:type ;
				  sh:name "type" ;
				  sh:order 8 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:supportedProtocols ;
				  sh:name "supported protocols" ;
				  sh:order 9 ;
				  sh:datatype xsd:string ] ;
	sh:property [ sh:path gax-resource:networkAdress ;
				  sh:name "network adress" ;
				  sh:order 10 ;
				  sh:datatype xsd:float ] ;
.

gax-validation:MeasureShape
	a sh:NodeShape ;
	sh:targetClass gax-core:Measure ;
	sh:property [ sh:path gax-core:value ;
				  sh:name "value" ;
				  sh:order 1 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:float ] ;
	sh:property [ sh:path gax-core:unit ;
				  sh:name "unit" ;
				  sh:order 2 ;
				  sh:minCount 1 ;
				  sh:maxCount 1 ;
				  sh:datatype xsd:string ] ;
.
